{
  "name": "saladbar-core",
  "version": "0.1.3",
  "description": "Core helper functions for building composable dom functions.",
  "main": "lib/cjs/index.js",
  "keywords": [],
  "module": "lib/es/index.js",
  "repository": "https://github.com/wking-io/saladbar",
  "author": "Will King <contact@wking.io> (https://www.wking.io/)",
  "license": "Apache-2.0",
  "scripts": {
    "build": "node ./tools/build.js",
    "watch": "babel ./modules -d . --ignore __tests__ --watch",
    "prepublishOnly": "node ./tools/build.js && yarn test:build",
    "clean": "git clean -fdX .",
    "lint": "eslint modules/**/*.js",
    "format": "prettier-eslint --trailing-comma es5 --single-quote --write 'modules/*/*.js'",
    "test": "babel-tape-runner modules/__tests__/*.test.js | tap-spec",
    "test:build": "babel-tape-runner __tests__/*.test.js | tap-spec"
  },
  "dependencies": {
    "ramda": "0.25.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-tape-runner": "2.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.8.0",
    "gzip-size": "4.1.0",
    "husky": "0.14.3",
    "jsdom": "11.6.2",
    "lint-staged": "6.1.0",
    "prettier": "1.10.2",
    "prettier-eslint-cli": "4.7.1",
    "pretty-bytes": "4.0.2",
    "rollup": "0.56.0",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-commonjs": "8.3.0",
    "rollup-plugin-includepaths": "0.2.2",
    "rollup-plugin-node-resolve": "3.0.3",
    "rollup-plugin-uglify": "3.0.0",
    "tap-spec": "4.1.1",
    "tape": "4.8.0"
  }
}
